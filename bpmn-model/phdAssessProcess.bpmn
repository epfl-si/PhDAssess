            &#34;show&#34;: true,             &#34;when&#34;: &#34;GeneralAppraisalOfTheProgress&#34;,             &#34;eq&#34;: &#34;unsatisfactory&#34;           },           &#34;type&#34;: &#34;htmlelement&#34;,           &#34;input&#34;: false,           &#34;tableView&#34;: false         },         {           &#34;label&#34;: &#34;Thesis Co-Director date of review:&#34;,           &#34;widget&#34;: {             &#34;type&#34;: &#34;calendar&#34;,             &#34;altInput&#34;: true,             &#34;allowInput&#34;: false,             &#34;clickOpens&#34;: true,             &#34;enableDate&#34;: true,             &#34;enableTime&#34;: false,             &#34;mode&#34;: &#34;single&#34;,             &#34;noCalendar&#34;: false,             &#34;format&#34;: &#34;dd.MM.yyyy&#34;,             &#34;dateFormat&#34;: &#34;yyyy-MM-dd&#34;,             &#34;useLocaleSettings&#34;: false,             &#34;hourIncrement&#34;: 1,             &#34;minuteIncrement&#34;: 5,             &#34;time_24hr&#34;: false,             &#34;saveAs&#34;: &#34;date&#34;,             &#34;locale&#34;: &#34;en&#34;           },           &#34;tableView&#34;: true,           &#34;customDefaultValue&#34;: &#34;value = moment().format(&#39;YYYY-MM-DD&#39;);&#34;,           &#34;validate&#34;: {             &#34;required&#34;: true           },           &#34;key&#34;: &#34;thesisCoDirectorDateOfReview&#34;,           &#34;type&#34;: &#34;textfield&#34;,           &#34;input&#34;: true         },         {           &#34;label&#34;: &#34;Thesis Co-Director review&#34;,           &#34;optionsLabelPosition&#34;: &#34;right&#34;,           &#34;inline&#34;: false,           &#34;tableView&#34;: false,           &#34;values&#34;: [             {               &#34;label&#34;: &#34;I agree&#34;,               &#34;value&#34;: &#34;Thesis co-director agrees&#34;,               &#34;shortcut&#34;: &#34;&#34;             },             {               &#34;label&#34;: &#34;I do not agree&#34;,    