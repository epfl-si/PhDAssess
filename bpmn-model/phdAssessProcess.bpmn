 ….&#60;/i&#62;&#60;/p&#62;&#34;,           &#34;refreshOnChange&#34;: false,           &#34;key&#34;: &#34;html6&#34;,           &#34;type&#34;: &#34;htmlelement&#34;,           &#34;input&#34;: false,           &#34;tableView&#34;: false         },         {           &#34;label&#34;: &#34;Phd Comment&#34;,           &#34;autoExpand&#34;: false,           &#34;disabled&#34;: true,           &#34;tableView&#34;: true,           &#34;validate&#34;: {             &#34;required&#34;: true           },           &#34;key&#34;: &#34;phdComment5&#34;,           &#34;type&#34;: &#34;textarea&#34;,           &#34;input&#34;: true         },         {           &#34;label&#34;: &#34;Thesis Director Comment&#34;,           &#34;autoExpand&#34;: false,           &#34;disabled&#34;: true,           &#34;tableView&#34;: true,           &#34;validate&#34;: {             &#34;required&#34;: true           },           &#34;key&#34;: &#34;thesisDirectorComment5&#34;,           &#34;type&#34;: &#34;textarea&#34;,           &#34;input&#34;: true         },         {           &#34;label&#34;: &#34;Common Comment&#34;,           &#34;autoExpand&#34;: false,           &#34;disabled&#34;: true,           &#34;tableView&#34;: true,           &#34;validate&#34;: {             &#34;required&#34;: true           },           &#34;key&#34;: &#34;commonComment5&#34;,           &#34;type&#34;: &#34;textarea&#34;,           &#34;input&#34;: true         },         {           &#34;label&#34;: &#34;HTML&#34;,           &#34;attrs&#34;: [             {               &#34;attr&#34;: &#34;&#34;,               &#34;value&#34;: &#34;&#34;             }           ],           &#34;content&#34;: &#34;&#60;p&#62;&#60;strong&#62;6. Any other specific points&#60;/strong&#62;&#60;br&#62;&#34;,           &#34;refreshOnChange&#34;: false,           &#34;key&#34;: &#34;html7&#34;,           &#34;type&#34;: &#34;htmlelement&#34;,           &#34;input&#34;: false,           &#34;tableView&#34;: false         },         {           &#34;label&#34;: &#34;Phd Comment&#34;,           &#34;autoExpand&#34;: false,           &#34;disabled&#34;: true,           &#34;tableView&#34;: true,           &#34;key&#34;: &#34;phdComment6&#34;,           &#34;type&#34;: &#34;textarea&#34;,           &#34;input&#34;: true         },         {           &#34;label&#34;: &#34;Thesis Director Comment&#34;,           &#34;autoExpand&#34;: false,           &#34;disabled&#34;: true,           &#34;tableView&#34;: true,           &#34;key&#34;: &#34;thesisDirectorComment6&#34;,           &#34;type&#34;: &#34;textarea&#34;,           &#34;input&#34;: true         },         {           &#34;label&#34;: &#34;Common Comment&#34;,           &#34;autoExpand&#34;: false,           &#34;disabled&#34;: true,           &#34;tableView&#34;: true,           &#34;key&#34;: &#34;commonComment6&#34;,           &#34;type&#34;: &#34;textarea&#34;,           &#34;input&#34;: true         }       ]     },     {       &#34;title&#34;: &#34;Section D: Overall appraisal and signatures&#34;,       &#34;collapsible&#34;: false,       &#34;key&#34;: &#34;sectionDOverallAppraisalAndSignatures&#34;,       &#34;type&#34;: &#34;panel&#34;,       &#34;label&#34;: &#34;Panel&#34;,       &#34;input&#34;: false,       &#34;tableView&#34;: false,       &#34;components&#34;: [         {           &#34;label&#34;: &#34;HTML&#34;,           &#34;attrs&#34;: [             {               &#34;attr&#34;: &#34;&#34;,               &#34;value&#34;: &#34;&#34;             }           ],           &#34;content&#34;: &#34;&#60;p&#62;By signing the AR the student and the TD(s) agree that the discussion regarding the achieved and planned objectives as well as the progress assessment satisfy and are approved by both parties.&#60;/p&#62;\n&#60;p&#62;&#60;strong&#62;Notes&#60;/strong&#62;&#60;/p&#62;\n&#60;p&#62;&#60;strong&#62;For the PhD student: &#60;/strong&#62;&#60;&#38;nbsp;Your thesis director is responsible for guiding you in your research and for coaching you to reach the scientific level of a PhD. The critical feedback of your thesis director plays a key role in this process and thus also for your success beyond. Approval of the AR indicates that you have discussed with your thesis director and understand the expectations for improvement in the areas identified, and that you agree on the plan to reach these milestones and to assess your progress.&#60;/p&#62;\n&#60;p&#62;&#60;strong&#62;For the thesis director:&#60;/strong&#62;Your signature indicates that you have discussed all the above points with the student and considered his/her feedback regarding any potential problem, you have explained your expectations for improvement, when appropriate, including mechanisms for intermediate progress assessments, and you have discussed a plan for timely publication of the results and for thesis defence.&#60;/p&#62;&#34;,           &#34;refreshOnChange&#34;: false,           &#34;key&#34;: &#34;html&#34;,           &#34;type&#34;: &#34;htmlelement&#34;,           &#34;input&#34;: false,           &#34;tableView&#34;: false         },         {           &#34;label&#34;: &#34;&#60;b&#62;General appraisal of the progress&#60;/b&#62;&#34;,           &#34;optionsLabelPosition&#34;: &#34;right&#34;,           &#34;inline&#34;: false,           &#34;disabled&#34;: true,           &#34;tableView&#34;: false,           &#34;values&#34;: [             {               &#34;label&#34;: &#34;meets expectations&#34;,               &#34;value&#34;: &#34;meetsExpectations&#34;,               &#34;shortcut&#34;: &#34;&#34;             },             {               &#34;label&#34;: &#34;needs some improvement&#34;,               &#34;value&#34;: &#34;needsSomeImprovement&#34;,               &#34;shortcut&#34;: &#34;&#34;             },             {               &#34;label&#34;: &#34;unsatisfactory&#34;,               &#34;value&#34;: &#34;unsatisfactory&#34;,               &#34;shortcut&#34;: &#34;&#34;             } 